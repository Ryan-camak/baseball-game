{
  "name": "Java Spring Boot Development",
  "image": "mcr.microsoft.com/devcontainers/java:17", // 使用預裝 Java 17 的映像
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",      // Java Extension Pack
        "Pivotal.vscode-boot-dashboard"  // Spring Boot Dashboard
      ]
    }
  },
  // 可以添加其他配置，例如轉發端口
  "forwardPorts": [8080]
}